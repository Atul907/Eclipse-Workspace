package impPatterns;

public class HollowShape {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int r,c;
		
		for(r=1;r<=5;r++) {
			for(c=1;c<=5;c++) {
				if(r>1 && c>1 && r<5 && c<5) {
					System.out.print(" ");
				}
				System.out.print("*");
			}
			System.out.println();
		}
		
	}

}
